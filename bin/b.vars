VERSION="1.6.2"
BUILDTIME=`date -u +%FT%T%Z`
BUILDHASH=`git rev-parse HEAD`
STATUS=`git status --porcelain`
if [ "$STATUS" ]; then
  BUILDHASH="$BUILDHASH*"
fi

CGO_ENABLED=0
LDFLAGS="-X rakewire/model.Version=${VERSION} -X rakewire/model.BuildTime=${BUILDTIME} -X rakewire/model.BuildHash=${BUILDHASH}"
